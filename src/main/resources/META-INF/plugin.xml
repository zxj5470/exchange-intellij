<idea-plugin>
    <id>com.github.zxj5470.exchange-intellij</id>
    <name>Exchange Code block</name>
    <vendor email="zxj5470@foxmail.com" url="http://github.com/zxj5470">zxj5470</vendor>
    <idea-version since-build="139"/>
    <extensions defaultExtensionNs="com.intellij">
    </extensions>
    <actions>
        <action class="com.github.zxj5470.exchangeij.ActionCopy"
                id="com.github.zxj5470.exchangeij.ActionCopy">
            <keyboard-shortcut first-keystroke="ctrl alt C" keymap="$default"/>
        </action>
        <action class="com.github.zxj5470.exchangeij.ActionV"
                id="com.github.zxj5470.exchangeij.ActionV">
            <keyboard-shortcut first-keystroke="ctrl alt V" keymap="$default"/>
        </action>
    </actions>
</idea-plugin>